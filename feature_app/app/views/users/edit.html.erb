<h1> Edit User</h1>
<%= form_for @user do |f| %>
 <p> 
  <%= f.label :first_name %> <br/>
  <%= f.text_field :first_name %> <br/>
 </p>

 <p> 
  <%= f.label :last_name %> <br/>
  <%= f.text_field :last_name %> <br/>
 </p>

 
  <p> 
  <%= f.label :email %> <br/>
  <%= f.text_area :email %> <br/>
 </p>
 <p>
     <%= f.select :active, options_for_select(['true', 'false'], 'true'), {:include_blank => "Select One"} %> <br>
 </p>
 
 <p>
 <%= f.submit "Update User"%>
 </p>
<% end %>
<p><%= link_to "Go Back",user_path %>