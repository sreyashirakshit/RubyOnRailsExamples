<h1> Users Index</h1>

<% @users.each do |user| %>
 <h3> <%= user.first_name %> , <%= user.last_name %></h3>
 <p>  <%= user.email %>
 <%= link_to "Show", user_path(user)%>
 <%= link_to 'Destroy', user_path(user), method: :delete, data: {confirm: "Are you sure?"} %>
 </p>
 <% end %>
 <%= link_to "New", new_user_path%>